{
    "pointSize": 1.0,
    "color": "rgba(10, 180, 255, 1)",
    "labelColor": "rgba(180, 180, 255, 1)",
    "labelOutlineWidth": 4,
    "_labelStyle": "LabelStyle.FILL_AND_OUTLINE",
    "labelStyle": 2,
    "labelOutlineColor": "color('black')",
    "_labelHorizontalOrigin": "HorizontalOrigin.CENTER",
    "labelHorizontalOrigin": 0,
    "pixelOffset": "[0, -15]",
    "anchorLineEnabled": "true",
    "heightOffset": {
      "conditions": [
        ["${size} >= 9", "1000"],
        ["${size} >= 7", "600"],
        ["${size} >= 5", "250"],
        ["${size} >= 3", "100"],
        ["true", "50"]
      ]
    },
    "anchorLineColor": "rgba(255, 255, 255, 1)",
    "distanceDisplayCondition": "vec2(10000, 1e8)",
    "_verticalOrigin": "VerticalOrigin.BOTTOM",
    "verticalOrigin": 1,
    "translucencyByDistance": "vec4(10000, 0,  20000, 1)",
    "font": {
      "conditions": [
        ["${size} >= 9", "22px sans-serif"],
        ["${size} >= 7", "18px sans-serif"],
        ["${size} >= 5", "14px sans-serif"],
        ["${size} >= 3", "10px sans-serif"],
        [true, "6px sans-serif"]
      ]
    },
    "labelText": "${title}"
}
